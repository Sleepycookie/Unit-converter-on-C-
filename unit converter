        private void lstTemperatureFrom_SelectedIndexChanged(object sender, EventArgs e)
        {

        }

        private void btnTemperatureTo_Click(object sender, EventArgs e)
        {
            if (lstTemperatureFrom.SelectedItem == "Celsius" && lstTemperatureTo.SelectedItem == "Celsius")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * 1);
            }
            if (lstTemperatureFrom.SelectedItem == "Celsius" && lstTemperatureTo.SelectedItem == "Kelvin")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * 1+273.15);
            }
            if (lstTemperatureFrom.SelectedItem == "Celsius" && lstTemperatureTo.SelectedItem == "Fahrenheit")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * 1 *9/5+32);
            }
            if (lstTemperatureFrom.SelectedItem == "Kelvin" && lstTemperatureTo.SelectedItem == "Celsius")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * 1 - 273.15);
            }
            if (lstTemperatureFrom.SelectedItem == "Kelvin" && lstTemperatureTo.SelectedItem == "Kelvin")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * 1);
            }
            if (lstTemperatureFrom.SelectedItem == "Kelvin" && lstTemperatureTo.SelectedItem == "Fahrenheit")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * (1-273.15) * 9/5 +32);
            }
            if (lstTemperatureFrom.SelectedItem == "Fahrenheit" && lstTemperatureTo.SelectedItem == "Celsius")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * (1 - 32) * 5 / 9);
            }
            if (lstTemperatureFrom.SelectedItem == "Fahrenheit" && lstTemperatureTo.SelectedItem == "Kelvin")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * (1 - 32) * 5/9 + 273.15);
            }
            if (lstTemperatureFrom.SelectedItem == "Fahrenheit" && lstTemperatureTo.SelectedItem == "Fahrenheit")
            {
                txtTemperatureTo.Text = Convert.ToString(Convert.ToDouble(txtTemperatureFrom.Text) * 1);
            }
        }

