        private void btnWeightTo_Click(object sender, EventArgs e)
        {
            if (lstWeightFrom.SelectedItem == "Milligram" && lstWeightTo.SelectedItem == "Milligram")
            {
                txtWeightTo.Text = Convert.ToString(Convert.ToDouble(txtWeightFrom.Text) * 1);
            }
            if (lstWeightFrom.SelectedItem == "Milligram" && lstWeightTo.SelectedItem == "Gram")
            {
                txtWeightTo.Text = Convert.ToString(Convert.ToDouble(txtWeightFrom.Text) / 1000);
            }
            if (lstWeightFrom.SelectedItem == "Milligram" && lstWeightTo.SelectedItem == "Kilogram")
            {
                txtWeightTo.Text = Convert.ToString(Convert.ToDouble(txtWeightFrom.Text) * 0.000001);
            }
            if (lstWeightFrom.SelectedItem == "Milligram" && lstWeightTo.SelectedItem == "Pound")
            {
                txtWeightTo.Text = Convert.ToString(Convert.ToDouble(txtWeightFrom.Text) / 453592.37);
            }
            if (lstWeightFrom.SelectedItem == "Milligram" && lstWeightTo.SelectedItem == "Ounce")
            {
                txtWeightTo.Text = Convert.ToString(Convert.ToDouble(txtWeightFrom.Text) / 28349.523);
            }
            if (lstWeightFrom.SelectedItem == "Milligram" && lstWeightTo.SelectedItem == "Carat")
            {
                txtWeightTo.Text = Convert.ToString(Convert.ToDouble(txtWeightFrom.Text) / 200);
            }
            if (lstWeightFrom.SelectedItem == "Milligram" && lstWeightTo.SelectedItem == "Ton")
            {
