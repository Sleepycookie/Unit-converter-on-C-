            }

            if (lstLengthFrom.SelectedItem == "Meter" && lstLengthTo.SelectedItem == "Meter")
                {
                    txtLengthTo.Text = Convert.ToString(Convert.ToDouble(txtLengthFrom.Text) * 1);

                }
        

